@inject Microsoft.AspNetCore.Hosting.IHostingEnvironment hostingEnv
@using Microsoft.AspNetCore.Hosting
<!DOCTYPE html>
<html lang="en" data-ng-app="app" id="ng-app">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta id="viewport" name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    <link rel="shortcut icon"  asp-href-include="~/assets/images/favicon.ico" type="image/x-icon" />
    <link rel="mask-icon" asp-href-include="~/assets/images/gov.uk_logotype_crown.svg" color="#0b0c0c">
    <title>@ViewData["Title"] - Calculate funding</title>
    @if (hostingEnv.IsDevelopment())
    {
        <link href="~/assets/libs/css/bootstrap.css" rel="stylesheet" type="text/css" />
        @*<link href="~/assets/libs/css/summernote.css" rel="stylesheet" />*@
        <link rel="stylesheet" href="~/libs/govuk_bootstrap/govuk_bootstrap.css" type="text/css" />
        <link rel="stylesheet" href="~/assets/css/main.css" type="text/css" />

    }
    else
    {
        <link rel="stylesheet" asp-href-include="~/assetscss/libraries-*.min.css" type="text/css" />
        <link rel="stylesheet" asp-href-include="~/assetscss/main-*.min.css" type="text/css" />
    }

    @RenderSection("styles", false)
</head>
<body id="top">
    <header role="banner" id="global-header" class="with-proposition">
        <div class="header-wrapper">
            <div class="header-global">
                <div class="header-logo">
                    <a href="https://www.gov.uk" title="Go to the GOV.UK homepage" id="logo" class="content">
                        <img src="~/assets/images/gov.uk_logotype_crown_invert_trans.png" width="35" height="31" alt=""> GOV.UK
                    </a>
                </div>
            </div>
            <div class="header-proposition">
                <div class="content">
                    <a href="#proposition-links" class="js-header-toggle menu">Menu</a>
                    <nav id="proposition-menu">

                        <a href="/" id="proposition-name">Calculate Funding</a>

                        @*<ul id="proposition-links">
                                <li><a href="layout.html">Layout</a></li>
                                <li><a href="content.html">Content</a></li>
                                <li><a href="elements.html">Components</a></li>
                                <li><a href="developer_guide.html">Developer Guide</a></li>
                            </ul>*@
                    </nav>
                </div>
            </div>
        </div>

    </header>

    @if (IsSectionDefined("BannerLeft"))
    {
        <div class="container">
            <div class="row">
                <div class="col-xs-12 spacing-15">
                    <div class="govuk-beta-label">

                        <strong class="phase-tag">Beta</strong>
                        <span>
                            Complete our quick 5-question survey to <a target="_blank" rel="noopener noreferrer" href="#">help us improve our content</a>.

                        </span>
                    </div>

                </div>
            </div>
        </div>
        <section class="banner-container">
            <div class="container">
                <div class="row">
                    <div class="col-xs-9">
                        @RenderSection("BreadCrumb", false)

                        @RenderSection("BannerLeft", false)
                    </div>
                    <div class="col-xs-3">
                        <div class="banner-container-right">
                            <a href="#">Contact the Calculate Funding Service Team</a> with any comments or questions.
                            @RenderSection("BannerRight", false)
                        </div>

                    </div>
                </div>
                @if (IsSectionDefined("BannerMiddle"))
                {
                    <div class="row">
                        <div class="col-xs-12">
                            @RenderSection("BannerMiddle", false)
                        </div>
                    </div>

                }
            </div>
        </section>
    }
    else if (IsSectionDefined("FormBannerLeft"))
    {
        <div class="container form-banner-container-beta">
            <div class="row">
                <div class="col-xs-12 form-banner-container-beta-col">
                    <div class="govuk-beta-label">

                        <strong class="phase-tag">Beta</strong>
                        <span>
                            Complete our quick 5-question survey to <a target="_blank" rel="noopener noreferrer" href="#">help us improve our content</a>.

                        </span>
                    </div>
                    <hr class="spacing-15" />
                </div>
            </div>
        </div>

        <section class="form-banner-container">
            <div class="container">
                <div class="row">
                    <div class="col-xs-9">
                        <div class="form-banner-container-left">
                            @RenderSection("BreadCrumb", false)
                            @RenderSection("FormBannerLeft", false)
                        </div>
                    </div>
                    <div class="col-xs-3">
                        <div class="form-banner-container-right">
                            @if (!IsSectionDefined("FormBannerRight"))
                            {
                                <text><a href="#">Contact the Calculate Funding Service Team</a> with any comments or questions.</text>
                            }

                            @RenderSection("FormBannerRight", false)
                        </div>

                    </div>
                </div>
            </div>
        </section>
    }

    <main role="main">
        <div class="container">
            @*<div class="row">
                    <div class="col-xs-12">
                        <div class="phase-banner-alpha">
                            <p>
                                <strong class="phase-tag">ALPHA</strong>
                                <span>This is a new service – your <a href="https://designpatterns.hackpad.com/GOV.UK-elements-feedback-sKrDyQxcfA2">feedback</a> will help us to improve it.</span>
                            </p>
                        </div>
                    </div>
                </div>*@
            <!-- Breadcrumbs -->
            @*<div class="row">
                    <div class="col-xs-12">
                        <div class="breadcrumbs">
                            <ol>
                                <li><a href="index.html">GOV.UK Bootstrap</a></li>
                            </ol>
                        </div>
                    </div>
                </div>*@
            @RenderBody()

        </div>
    </main>
    <footer class="group js-footer" id="footer" role="contentinfo">
        <div class="footer-wrapper">
            <div class="footer-meta">
                <div class="footer-meta-inner">
                    <div class="open-government-licence">
                        <p class="logo"><a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence</a></p>
                        <p>All content is available under the <a href='https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/' rel='license'>Open Government Licence v3.0</a>, except where otherwise stated</p>
                    </div>
                </div>
                <div class="copyright">
                    <a href="http://www.nationalarchives.gov.uk/information-management/re-using-public-sector-information/copyright-and-re-use/crown-copyright/">© Crown copyright</a>
                </div>
            </div>
        </div>
    </footer>

    <script asp-src-include="~/assets/libs/js/jquery.min.js" asp-append-version="true" type="text/javascript"></script>
    <script asp-src-include="/assets/libs/js/bootstrap.js" asp-append-version="true" type="text/javascript"></script>
    @*<script src="/assets/libs/js/ace.js"></script>
    <script src="/assets/libs/js/keybinding-vim.js"></script>
    <script src="/assets/libs/js/keybinding-emacs.js"></script>
    <script src="/assets/libs/js/ext-language_tools.js"></script>
    <script src="/assets/libs/js/csharp.js"></script>
    <script src="/assets/libs/js/mode-csharp.js"></script>
    <script src="/assets/libs/js/mode-gherkin.js"></script>
    <script src="/assets/libs/js/mode-vbscript.js"></script>
    <script src="/assets/libs/js/summernote.min.js"></script>*@

    @if (!hostingEnv.IsDevelopment())
    {
        <script asp-src-include="~/assetsjs/main-*.js" type="text/javascript"></script>
    }
    else
    {
        <script src="~/assets/js/main.js" type="text/javascript"></script>
    }


    @RenderSection("Scripts", required: false)
</body>
</html>
