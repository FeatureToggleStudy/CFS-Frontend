<!DOCTYPE html>
<html lang="en" data-ng-app="app" id="ng-app">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta id="viewport" name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    <link rel="shortcut icon" href="assets/images/favicon.ico" type="image/x-icon" />
    <link rel="mask-icon" href="assets/images/gov.uk_logotype_crown.svg" color="#0b0c0c">
    <title>@ViewData["Title"] - Calculate funding</title>
    <link href="~/assets/libs/css/bootstrap.css" rel="stylesheet" />
    <link href="~/assets/libs/css/summernote.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/libs/govuk_bootstrap/govuk_bootstrap.css" type="text/css" />
    <link rel="stylesheet" href="~/assets/css/main.css" type="text/css" />
</head>
<body id="top">
    <header role="banner" id="global-header" class="with-proposition">
        <div class="header-wrapper">
            <div class="header-global">
                <div class="header-logo">
                    <a href="https://www.gov.uk" title="Go to the GOV.UK homepage" id="logo" class="content">
                        <img src="assets/images/gov.uk_logotype_crown_invert_trans.png" width="35" height="31" alt=""> GOV.UK
                    </a>
                </div>
            </div>
            <div class="header-proposition">
                <div class="content">
                    <a href="#proposition-links" class="js-header-toggle menu">Menu</a>
                    <nav id="proposition-menu">

                        <a href="/" id="proposition-name">Calculate Funding</a>

                        @*<ul id="proposition-links">
                                <li><a href="layout.html">Layout</a></li>
                                <li><a href="content.html">Content</a></li>
                                <li><a href="elements.html">Components</a></li>
                                <li><a href="developer_guide.html">Developer Guide</a></li>
                            </ul>*@
                    </nav>
                </div>
            </div>
        </div>
    </header>
    @*<section class="thin-top-banner">
    </section>*@
    <section class="banner-container">
        <div class="container">
            <div class="row">
                <div class="col-xs-9">
                    @RenderSection("BreadCrumb", false)
                    @RenderSection("BannerLeft", false)
                </div>
                <div class="col-xs-3">
                    <div class="banner-container-right">
                        <a href="#">Contact the Calculate Funding Service Team</a> with any comments or questions.
                        @*@RenderSection("BannerRight", false)*@
                    </div>

                </div>
            </div>
        </div>
    </section>

        <main role="main">
            <div class="container">
             
                @RenderBody()

            </div>
        </main>
        <footer class="group js-footer" id="footer" role="contentinfo">
            <div class="footer-wrapper">
                <div class="footer-meta">
                    <div class="footer-meta-inner">
                        <div class="open-government-licence">
                            <p class="logo"><a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence</a></p>
                            <p>All content is available under the <a href='https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/' rel='license'>Open Government Licence v3.0</a>, except where otherwise stated</p>
                        </div>
                    </div>
                    <div class="copyright">
                        <a href="http://www.nationalarchives.gov.uk/information-management/re-using-public-sector-information/copyright-and-re-use/crown-copyright/">© Crown copyright</a>
                    </div>
                </div>
            </div>
        </footer>

        <script src="/assets/libs/js/jquery.min.js"></script>
        <script src="/assets/libs/js/bootstrap.js"></script>
        <script src="/assets/libs/js/ace.js"></script>
        <script src="/assets/libs/js/keybinding-vim.js"></script>
        <script src="/assets/libs/js/keybinding-emacs.js"></script>
        <script src="/assets/libs/js/ext-language_tools.js"></script>
        <script src="/assets/libs/js/csharp.js"></script>
        <script src="/assets/libs/js/mode-csharp.js"></script>
        <script src="/assets/libs/js/mode-gherkin.js"></script>
        <script src="/assets/libs/js/mode-vbscript.js"></script>
        <script src="/assets/libs/js/summernote.min.js"></script>
        <script src="/assets/js/main.js"></script>

        @RenderSection("Scripts", required: false)
</body>
</html>
